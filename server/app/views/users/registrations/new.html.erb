<% content_for :navbar do %>
  <%= link_to raw('<i class="icon-chevron-left icon-fixed-width"></i>'), '#', :class => 'btn btn-danger' %>
  <%= link_to 'Home', root_path, :class => 'btn btn-danger' %>
<% end %>

<% fields = [
  :name,
  :address,
  :city,
  :province,
  :country,
  :phone
] %>

<%= form_for @user, :html => {:class => 'form-horizontal'} do |f| %>
  <div class="row">
    <div class="span6">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :first_name} %>
    </div>
    <div class="span6">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :last_name} %>
    </div>
  </div>
  <div class="row">
    <div class="span12">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :address} %>
    </div>
  </div>
  <div class="row">
    <div class="span4">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :city} %>
    </div>
    <div class="span4">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :province} %>
    </div>
    <div class="span4">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :country} %>
    </div>
  </div>
  <div class="row">
    <div class="span12">
      <%= render :partial => 'users/registrations/new_form_field',
                 :locals => {:f => f, :field => :phone} %>
    </div>
  </div>
<% end %>

